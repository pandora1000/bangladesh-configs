{
   "author": "Brian Birtles",
   "background": {
      "scripts": [ "10ten-ja-background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "images/10ten-disabled-16.png",
         "32": "images/10ten-disabled-32.png",
         "48": "images/10ten-disabled-48.png"
      },
      "default_title": "__MSG_command_toggle_disabled__"
   },
   "commands": {
      "_execute_browser_action": {
         "description": "__MSG_command_toggle_description__",
         "suggested_key": {
            "default": "Alt+R"
         }
      }
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "10ten-ja-content.js" ],
      "match_about_blank": true,
      "matches": [ "http://*/*", "https://*/*", "file:///*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "10ten-ja-gdocs-bootstrap.js" ],
      "matches": [ "https://docs.google.com/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_extension_description__",
   "differential_fingerprint": "1.dcd66738ce0e22b706f71e6eca6f086eed433746239adc8a1ae26b3d4262780e",
   "icons": {
      "128": "images/10ten-ja-reader-128.png",
      "16": "images/10ten-ja-reader-16.png",
      "32": "images/10ten-ja-reader-32.png",
      "48": "images/10ten-ja-reader-48.png",
      "96": "images/10ten-ja-reader-96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "10ten Japanese Reader (Rikaichamp)",
   "offline_enabled": true,
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "alarms", "contextMenus", "storage", "unlimitedStorage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.13.5",
   "web_accessible_resources": [ "css/popup.css", "docs/introducing-the-mouse.html", "images/warning-red.svg", "images/warning-yellow.svg" ]
}
